  "bugs": {
    "url": "https://github.com/optsoldev/components-frontend-react/issues"
  },
  "homepage": "https://github.com/optsoldev/components-frontend-react#readme",
  "devDependencies": {
    "lerna": "^6.4.1",
    "husky": "^8.0.3"
  }
}
